<!--pages/do/do.wxml-->

<view> 
<text>名称:{{testRecord.title}}</text>

<view wx:if="{{currentQuestion.type=='single'}}">
<single question="{{currentQuestion}}"/>
</view>
<view wx:if="{{currentQuestion.type=='mutiply'}}">
<mutiply question="{{currentQuestion}}"/>

</view>

<button type="default" size="{{defaultSize}}" loading="{{loading}}" plain="{{plain}}"
        disabled="{{disabled}}" bindtap="doTest" hover-class="other-button-hover"> 下一题 </button>

        
 </view>
